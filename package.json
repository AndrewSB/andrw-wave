{
  "name": "andrw-wave",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.3.0",
    "react-window-size": "^1.2.2",
    "viewport-units-buggyfill": "^0.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp build/index.html build/404.html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run-script build && npm run-script deploy-surge && npm run-script deploy-ghpages",
    "deploy-surge": "npx surge build andrew.energy",
    "deploy-ghpages": "npx gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
