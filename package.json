{
  "name": "andrw-wave",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-window-size": "^1.0.1",
    "viewport-units-buggyfill": "^0.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp build/index.html build/404.html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "react-scripts build && react-scripts deploy-surge && react-scripts deploy-ghpages",
    "deploy-surge": "npx surge build andrew.energy",
    "deploy-ghpages": "npx gh-pages -d build"
  }
}
